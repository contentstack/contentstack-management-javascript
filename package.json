{
  "name": "contentstack-management-api",
  "version": "0.0.1",
  "description": "The Content Management API is used to manage the content of your Contentstack account",
  "main": "Contentstack.js",
  "directories": {
    "test": "test"
  },
  "nyc": {
    "exclude": [
      "**/test"
    ]
  },
  "scripts": {
    "clean": "rimraf coverage && rimraf dist",
    "build": "npm run clean && npm run build:es5 && npm run build:es-modules",
    "build:es5": "BABEL_ENV=es5 babel lib -d dist/es5",
    "build:es-modules": "BABEL_ENV=es-modules babel lib -d dist/es-modules",
    "test": "BABEL_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register ./test/api ./test/unit  -t 30000 --reporter mochawesome",
    "test:debug": "BABEL_ENV=test mocha debug --require @babel/register ./test",
    "lint": "eslint lib test",
    "format": "eslint --fix lib test",
    "pretest": "rimraf coverage && npm run lint",
    "precommit": "npm run lint",
    "prepush": "npm run test:unit",
    "generate:docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "webpack": "npx webpack"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Contentstack",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.5.5",
    "@babel/register": "^7.5.5",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "axios-retry": "^3.1.2",
    "babel-plugin-rewire": "^1.2.0",
    "docdash": "^1.1.1",
    "eslint-config-standard": "^13.0.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.3",
    "jsdoc-to-markdown": "^5.0.0",
    "lodash": "^4.17.15",
    "mochawesome": "^4.0.1",
    "nyc": "^14.1.1",
    "require-all": "^3.0.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "webpack": "^4.36.1"
  },
  "keywords": [
    "contentstack management api",
    "contentstack",
    "management api"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "webpack-cli": "^3.3.6"
  },
  "homepage": "https://www.contentstack.com/docs/apis/content-management-api"
}
